              <li class="breadcrumb-item"><b>Username</b>: <%= logs[i].username%> <br> <b>Password</b>: <%= logs[i].password%></li>
            </ol>
          </nav>
        </div>
    </div>
    <div class="row m-3">
        <%for(let j=0;j<logs[i].links.length;j++){%>
        <div class="col-sm-3 mb-4">
          <div class="card">
            <div class="card-body">
              <p class="card-text">üë§<b>Username</b>: <%=logs[i].links[j].username%></p>
              <p class="card-text">üóùÔ∏è<b>Password</b>: <%=logs[i].links[j].password%></p>
              <a href="<%=logs[i].links[j].url%>" style="text-decoration: none;"></a><button type="button" class="btn btn-outline-primary ms-1 px-3">Visit website‚ÜóÔ∏è</button></a>
            </div>
          </div>
        </div>
        <%}%>
    </div>
    <%}%>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</html>